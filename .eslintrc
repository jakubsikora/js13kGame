{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "extends": "airbnb-base",
  "rules": {
    "import/imports-first": 0,
    "max-len": [2, {
      "code": 80,
      "tabWidth": 2,
      "ignoreUrls": true
    }],
    "eol-last": 2,
    "no-underscore-dangle": 0,
    "consistent-return": 1,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "arrow-parens": 0
  },
  "globals": {
    "window": false,
    "document": false
  }
}